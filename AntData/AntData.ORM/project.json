{
  "name": "AntData.Core",
  "version": "1.0.0",
  "authors": [ "nainaigu" ],
  "description": "AntData.Orm For Dotnet Core",
  "packOptions": {
    "licenseUrl": "https://github.com/yuzd/AntData.ORM/blob/master/MIT-LICENSE.txt",
    "projectUrl": "https://github.com/yuzd/AntData.ORM",
    "tags": [
      "linq",
      "ORM",
      "database",
      "DB",
      "SQL",
      "SqlServer"
    ]
  },
  "buildOptions": {
    "outputName": "AntData.ORM",
    "embed": [ "**/*.resx", "**/*.txt" ],
